[core]
	excludesfile = /Users/alexandr/.gitignore_global
	autocrlf = input
[difftool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/araxiscompare-w.sh -2 \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/araxisgitmerge-w.sh \"$REMOTE\" \"$BASE\" \"$LOCAL\" \"$MERGED\"
	trustExitCode = true
[user]
	name = Alexandr Cherepanov
	email = xandr.cherepanov@gmail.com
[color]
	ui = true
[push]
	default = simple
[credential]
	helper = osxkeychain
[commit]
	template = /Users/alexandr/.stCommitMsg
[includeIf "gitdir:~/Alfa/"]
  path = .gitconfig_alfa
